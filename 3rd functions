
//const

const printMyName=(name)=>{
  console.log(name);
}

printMyName('Rumeysa');

Output:
"Rumejsa"
//



//normal function

function printMyName(name){
  console.log(name);
}

printMyName('Rumeysa');

Output:
"Rumejsa"
//









// 2 parametra const

const printMyName=(name, age)=>{
  console.log(name, age);
}

printMyName("Rumeysa", 23);

Output:
"Rumejsa"
23
